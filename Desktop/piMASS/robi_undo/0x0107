  c1	 
 name="paint"
 pix ="

#version 120																
varying vec2 fr_uv;														
uniform sampler2D	tex0;							
uniform sampler2D	tex1;					
uniform vec2		aTX[8];		
uniform vec4 		aCNL[8];											
void main()																
{																		
	if( length(fr_uv*aTX[0]-aCNL[0].xy) < (aCNL[0].w+16) )
		gl_FragColor = vec4(1,0.5,0,1);
	else
		gl_FragColor = vec4(0,0.5,1,1); //texture2D( tex0, fr_uv );
	
}																	
 	
