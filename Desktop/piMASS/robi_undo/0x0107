 c2	  
 name="pena"
 pix ="

#version 120																
varying vec2 fr_uv;														
uniform sampler2D	tex0;							
uniform sampler2D	tex1;				
uniform vec2		aTX[8];		
uniform vec4 		aCNL[8];											
void main()																
{																		
	gl_FragColor =	texture2D( tex0, fr_uv )
					+aCNL[0].xyxy;
 
 					
	if( gl_FragColor.a < (1/0x100) )									
 		discard;														
}																		
 	
