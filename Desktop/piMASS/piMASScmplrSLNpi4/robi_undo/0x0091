 R# o# b# i#                                                                  b0	 																	  d0	  																							 e0	 										 f0	 											 g0	  																	 a1	  
 telnet = 6001;
 rout = 0x10003;
 rinp = 0x10002;
 slmp = "192.168.1.90 10000"
 cam = 1
 pic = "cam1"; 
 
 bob = "exp";
 f = fps; 
  b1	 
 pic = 	//"cam1";
 	"gui";
 unsel = 1;
  c1	 
 name="head"
 pix ="

#version 120																
varying vec2 fr_uv;														
uniform sampler2D	tex0,	// cam1
					tex1;	// penna
					//tex2;	// space
uniform vec2		tgPX;					
uniform vec2		aTX[8];		
uniform vec4 		aCNL[8];
#define v2base	vec2(20.0, 28.0)
#define v2out	vec2(20.0, 10.0)
#define v2frnt	vec2(10.0, 10.0)
#define v2back	vec2(10.0, 15.0)
#define v2fbWH	vec2(10.0, 5.0)
#define v2spc	vec2(10.0,20.0)
#define v2sLU 	vec2(2.0,12.0)
#define v2rg	vec2( 8.0, 8.0)
#define v2bg	vec2(-8.0, 8.0)
#define v2br	vec2(-8.0,-8.0)
#define v2rgB	(vec2( 8.0, 8.0)/v2base)
#define v2bgB	(vec2(-8.0, 8.0)/v2base)
#define v2brB	(vec2(-8.0,-8.0)/v2base)
#define v2outB	(v2out/v2base)
#define v2spcB	(v2spc/v2base)
#define v2frntB (v2frnt/v2base)
#define v2backB (v2back/v2base)
#define v2fbWHB (v2fbWH/v2base)
#define v2sLUb	(v2sLU/v2base)	
#define	v2src	vec2(320,160)
#define spcOF	255	 
void main()																
{
	vec2	fUV = fr_uv*vec2(1,-1)+vec2(0,1);
	gl_FragColor = vec4( texture2D( tex1, fUV ).rgb, 1.0 );	// penna
	vec3	src = vec3(0,0,0),
			spc = vec3(0,0,0);
	
	if( fUV.y < v2outB.y )
	{
		vec2	srcUV = fUV/v2outB;
		src	= floor(texture2D( tex0, srcUV ).rgb*64.0)*4.0;
		
		vec2 	tg2x2 	= aTX[0]/2.0,
				tg5		= aTX[0]/5.0,
				pix2 	= floor(srcUV*tg2x2),			// 320x160
				ix22	= floor(fract(pix2/2.0)*2.0),	// 01010,01010
				ix5		= pix2/tg5,					// 01234
				ix501	= floor(fract(ix5/2.0)*2.0),// 01010
				
				ix50	= floor(ix5)-ix501,			// 00224		
				ix51	= floor(ix5)-1.0 + ix501, 	// -1 0 1 2 3
													//  0 1 1 3 3
				spcUV 	= vec2( 
							ix22.x < 1.0 ? ix50.x:ix51.x, 
							ix22.y < 1.0 ? ix50.y:ix51.y 
						); 
		//spcUV += v2sLUb*tgPX;
		spc	= texture2D( tex1, (vec2(spcOF+src.r,spcOF+src.g)+spcUV)/tgPX ).rgb
			* texture2D( tex1, (vec2(spcOF-src.b,spcOF+src.g)+spcUV)/tgPX ).rgb
			* texture2D( tex1, (vec2(spcOF-src.b,spcOF-src.r)+spcUV)/tgPX ).rgb;
		
		//spcUV -= v2sLUb*tgPX;
		float ix = spcUV.x + spcUV.y;
		gl_FragColor.rgb =  floor(fract( max( vec3(0), vec3( ix/2.0, ix/4.0, ix/8.0 )))*2.0 );
		
		/*if( length(spc-vec3(1,0,0))<1.0 )
 			spc = vec3(3,0,0);
 		else if( length(spc-vec3(0,1,0))<1.0 )
 			spc = vec3(0,3,0);
 		else if( length(spc-vec3(0,0,1))<1.0 )
 			spc = vec3(0,0,3);
 		else
 			spc = vec3(0,0,0);
 		
 		gl_FragColor.rgb = max( spc, gl_FragColor.rgb );*/
	}
	else if( fUV.y < v2spcB.y )
	{
		if( fUV.x < v2spcB.x )
		{
			spc.br = (fUV-v2spcB)/v2brB;
			if( max(spc.b,spc.r) < 1.0 )
				gl_FragColor.br += floor(spc.br*16.0)/48.0;
		} else {
			if( fUV.y < v2backB.y )
			{
				src = texture2D( tex0, (fUV-v2frntB)/v2fbWHB ).rgb/3.0;
				gl_FragColor.rgb = max( src, gl_FragColor.rgb );
			}
		}
	} else {
		if( fUV.x < v2spcB.x )
		{
			spc.bg = (fUV-v2spcB)/v2bgB;
			if( max(spc.b,spc.g) < 1.0 )
				gl_FragColor.bg += floor(spc.bg*16.0)/48.0;
		} else {
			spc.rg = (fUV-v2spcB)/v2rgB;
			if( max(spc.r,spc.g) < 1.0 )
				gl_FragColor.rg += floor(spc.rg*16.0)/48.0;
		}
	}
	// szálkereszt
	vec2 crs = abs( fUV*tgPX - aCNL[0].xy );
	gl_FragColor.b += 1 - min( 1, min(abs(crs.x),abs(crs.y)) );
}	
  "  
 
 pico = "exp" d1	 
 
 trg = "gui";
 trgw = 640;
 trgh = 896;
 
 pico="cam1";
 pici="penna";
 picII="space";
 cnlx = trgw*ix/iw;
 cnly = trgh*iy/ih;
 cnlz = 8;
 cnlw = cnlz*mlb;
 gpu="head";

 f = fps; 
  e1	 					 		
 pic = "penna"; //d2.trg;
 unsel = 1;  f1	 
 
 trg = "dilet"
 trgw = 256;
 trgh = 128;
 pico = "gui"
 pici = "dilet"
 gpu = "dilet"  g1	 

 name="dilet"
 pix = "
 #version 120																
varying vec2 fr_uv;														
uniform sampler2D	tex0,
					tex1;
uniform vec2		tgPX;					
uniform vec2		aTX[8];		
uniform vec4 		aCNL[8];											
void main()																
{	
	gl_FragColor = vec4(0,0,0,1);
	vec2	flpUV = fr_uv*vec2(1.0,-15.0/31) + vec2(0.0, 15.0/31);
 	vec2	flpUV1 = fr_uv*vec2(1.0,-1.0) + vec2(0.0, 1.0 ), 
 			off = 1.0/tgPX, uv;
	vec3 ize;
	gl_FragColor.rgb = texture2D( tex0, flpUV ).rgb;
 	for( int i = -2; i <= 2; ++i )
	for( int j = -2; j <= 2; ++j )
	{
		uv = vec2(i,j);
		ize = texture2D( tex0, flpUV+off*uv ).rgb*length(uv)/2.0;
		gl_FragColor.a += length(ize);
		gl_FragColor.rgb += ize*length(ize);
	}		
 	gl_FragColor /= gl_FragColor.a;
 
 /*if( 
 	max(
 			max(gl_FragColor.r, gl_FragColor.b),
 			
 			gl_FragColor.g
		) < 32.0/256.0 
 ) 
 	gl_FragColor *= 0.0;
 				  //*texture2D( tex1, flpUV1 ).rgb;*/
}
 "
