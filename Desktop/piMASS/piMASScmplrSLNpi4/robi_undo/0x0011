 c2	  
 name="pena"
pix ="

#version 120																
varying vec2 fr_uv;														
uniform sampler2D	tex0,	// space							
					tex1,	// maski
					tex2;	// src pl.: cam	
uniform vec2		tgPX;					
uniform vec2		aTX[8];		
uniform vec4 		aCNL[8];											
void main()																
{																		
	gl_FragColor = vec4(0,0,0,1);
 	vec2	flpUV = fr_uv*vec2(1.0,-1.0) + vec2(0.0,1.0),
 			crs = abs( flpUV*tgPX - aCNL[0].xy );
	vec3	a = vec3(0,0,0), 
			b = texture2D( tex1, flpUV ).rgb * min(1,aTX[1].x);				
	if( flpUV.y>(15.0/31.0) ) 
	if( flpUV.x<(16.0/30.0) ) 
	{
		a = texture2D( tex0, 
							(flpUV-vec2(0,15.0/31.0)) 
							/vec2(16.0/30.0,16.0/31.0)
					).rgb;
	}
	else if( flpUV.y>(22.0/31.0) ) 
	{
		vec2 crs2 = abs(
						flpUV*tgPX 
						- (aCNL[0].xy + tgPX*vec2(0,7.0/31.0))
 					);
		if( (length(crs2)/4.0) < 1 )
		{
			b = texture2D( tex2, 
							(flpUV-vec2(16.0/30.0,22.0/31.0))
							/vec2(14.0/30.0,7.0/31.0)
						).rgb;
		}
	} 
	
 	gl_FragColor.rgb = max( a, b );
	if( (length(crs)/4.0) < 1.0 )
		gl_FragColor.rgb += vec3( 0.0, 0.0, 1.0 );
}	
  "  
