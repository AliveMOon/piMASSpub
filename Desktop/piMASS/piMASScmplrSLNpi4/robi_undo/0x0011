// JOHN HS12:0000
// POS:  n:7
//	iXYZ  811.14mm,  469.00mm,  366.88mm 
//	oXYZ  400.00mm,    0.00mm,  700.00mm 
//	tXYZ  400.00mm,    0.00mm,  700.00mm;
// DIR:  n:4
//	iABC -180.00dg,    0.00dg,  -90.00dg 
//	oABC  180.00dg,    0.00dg,   90.00dg 
