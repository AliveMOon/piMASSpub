 R# o# b# i#                                                                  d0	  									 e0	 										 a1	  
 telnet = 6001;
 rout = 0x10003;
 rinp = 0x10002;
 slmp = "192.168.1.90 10000"
 cam = 1
 pic = "cam1"; 
 f = fps; 
  b1	 
 pic = 	//"cam1";
 		"gui";
 unsel = 1;
  c1	 
 name="head"
 pix ="

#version 120																
varying vec2 fr_uv;														
uniform sampler2D	tex0,
					tex1;
uniform vec2		tgPX;					
uniform vec2		aTX[8];		
uniform vec4 		aCNL[8];											
void main()																
{	
	gl_FragColor = vec4(0,0,0,1);
 	vec2	flpUV = fr_uv*vec2(1,-1) + vec2(0,1),
 			crs = abs(
 						flpUV*tgPX - aCNL[0].xy
 					);
	vec3	c = vec3(0,0,0), r = c, 
			a = texture2D( tex0, flpUV ).rgb,	// cam
			b = texture2D( tex1, flpUV ).rgb;	// mask
	if( flpUV.y < (15.0/31.0) )
	{
		c = texture2D(	tex0, 
						flpUV
						/ vec2(1.0,15.0/31.0)
					).rgb;
		r = texture2D(	tex1,
						(c.rg*vec2(8.0,8.0)+vec2(8.0,23.0))/vec2(30.0,31.0)
						//*vec2(1,-1) + vec2(0,1) 
					).rgb
			+texture2D(	tex1,
						(c.bg*vec2(-8.0,8.0)+vec2(8.0,23.0))/vec2(30.0,31.0)
						//*vec2(1,-1) + vec2(0,1) 
					).rgb
			+texture2D(	tex1,
						(c.br*vec2(-8.0,-8.0)+vec2(8.0,23.0))/vec2(30.0,31.0)
						//*vec2(1,-1) + vec2(0,1) 
					).rgb;
	}
	else if( flpUV.x < 16.0/30.0 )
	{
		c.rg  = max(vec2(0.0,0.0), (flpUV*30.0 - vec2(8.0,23.0))*vec2( 1.0, 1.0))/8.0;
		c.bg += max(vec2(0.0,0.0), (flpUV*30.0 - vec2(8.0,23.0))*vec2(-1.0, 1.0))/8.0;
		c.br += max(vec2(0.0,0.0), (flpUV*30.0 - vec2(8.0,23.0))*vec2(-1.0,-1.0))/8.0;
 	}
	else if( flpUV.y < (22.0/31.0) )
	{
		c = texture2D(	tex0, 
						(flpUV-vec2(16.0/30.0,15.0/31.0))
						/ vec2(14.0/30.0,7.0/31.0)
					).rgb*2.0;
	}
	gl_FragColor.rgb = (max( r,b*3.0)+c)/6.0;
 	gl_FragColor.b += 1 - min( 1, min(abs(crs.x),abs(crs.y)) );
 	 
 }	
  "  
