 c3	 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 

 
  
 
 
 
 
 
 
 
 
 
 
 
 
 
  a4	 
 
 name = "penna";
 pix = "
 #version 120
 varying vec2 fr_uv;
 uniform sampler2D	tex0,	//
                        tex1,	// draw
			tex2,	// cam1
                        tex3;	// mask
 uniform vec2		tgPX;
 uniform vec2		aTX[8];
 uniform vec4 		aCNL[8];
 vec2 dis( vec2 fUV, vec2 cnl ){
      return abs(fUV*tgPX - cnl );
 }
 float l( vec2 fUV, vec2 cnl ) {
       return length(dis(fUV,cnl));
 }
 void main()
 {
      vec2	fUV = fr_uv*vec2(1.0,-1.0) + vec2(0.0,1.0),
                cUV = aCNL[0].xy/tgPX,
		crs = abs(fUV-cUV)*tgPX,
                f2030 = floor(fUV*vec2(20,30)),
                c2030 = floor(cUV*vec2(20,30));

      vec4  pen = vec4(0,0,0,1),
           draw = (length(aTX[2])>0) ? texture2D(tex1,fUV) : pen;

      float f20 = dot(f2030,vec2(1,20)),
            c20 = dot(c2030,vec2(1,20)),
            //f3 = dot(floor(fUV*vec2(3,5)),vec2(1,3)),
            //c3 = dot(floor(cUV*vec2(3,5)),vec2(1,3)),
            dr3 = max(draw.x,max(draw.y,draw.z)),
            r = l(fUV,aCNL[0].xy);
      gl_FragColor = pen;


      if( f20 == 200 ) {
           gl_FragColor = vec4(0,0,0,1);
      } else if( f20 == 201 ) {
           gl_FragColor = vec4(1,0,0,1);
      } else if( f20 == 202 ) {
           gl_FragColor = vec4(0,1,0,1);
      } else if( f20 == 203 ) {
           gl_FragColor = vec4(1,1,0,1);
      } else if( f20 == 220 ) {
           gl_FragColor = vec4(0,0,1,1);
      } else if( f20 == 221 ) {
           gl_FragColor = vec4(1,0,1,1);
      } else if( f20 == 222 ) {
           gl_FragColor = vec4(0,1,1,1);
      } else if( f20 == 223 ) {
           gl_FragColor = vec4(1,1,1,1);
      } else if( f20 == 224 ) {
           if( c20 == 200 ) {
             gl_FragColor = vec4(0,0,0,1);
           } else if( c20 == 201 ) {
             gl_FragColor = vec4(1,0,0,1);
           } else if( c20 == 202 ) {
             gl_FragColor = vec4(0,1,0,1);
           } else if( c20 == 203 ) {
             gl_FragColor = vec4(1,1,0,1);
           } else if( c20 == 220 ) {
             gl_FragColor = vec4(0,0,1,1);
           } else if( c20 == 221 ) {
             gl_FragColor = vec4(1,0,1,1);
           } else if( c20 == 222 ) {
             gl_FragColor = vec4(0,1,1,1);
           } else if( c20 == 223 ) {
             gl_FragColor = vec4(1,1,1,1);
           } else {
             gl_FragColor = texture2D( tex1, fUV );
           }
      }
      else if( f20 == 599 ) {
             gl_FragColor = vec4(1,1,1,1);

      } else if(length(aTX[1]) > 0) {
          gl_FragColor = texture2D( tex1, fUV );
          pen = texture2D( tex1, vec2(4.5,11.5)/vec2(20.0,30.0) );

          if( dr3 < 0.77 ) {
              if( f2030.y < 10.0 ) {
                  gl_FragColor = (
                                     texture2D( tex2, fUV*vec2(1.0,3.0/1.0) )
                                     * vec4(0.5,0.75,0.5,1.0)
                                  );
              } else if( (f2030.y<15.0) && (f2030.x>10.0) ) {
                  gl_FragColor = (
                                     texture2D( tex2, (
                                                        (fUV-vec2(1.0/2.0,2.0/6.0))
                                                        *vec2(2.0/1.0,6.0/1.0)
                                                      )
                                     ) * vec4(0.5,0.75,0.5,1.0)
                                  );
              } else if( (f2030.y<20.0) && (f2030.x>10.0) ) {
                if( l(fUV-vec2(0,1.0/6.0),aCNL[0].xy) < aCNL[0].w )
                      gl_FragColor = (
                                         texture2D( tex2, (
                                                            (fUV-vec2(1.0/2.0,3.0/6.0))
                                                            *vec2(2.0/1.0,6.0/1.0)
                                                          )
                                         ) * vec4(0.5,0.75,0.5,1.0)
                                      );
                else
                     gl_FragColor = draw;
              }
          }
      }



      if( r < aCNL[0].w )
          gl_FragColor = pen;
      if(length(aTX[3]) > 0)
          gl_FragColor.rgb = (gl_FragColor.rgb + texture2D( tex3, fUV ).rgb)/2;
 }
 ";
 again = 1000;

 b4	 
 name = "combi";
 pix = "
 #version 120
 varying vec2 fr_uv;
 uniform sampler2D	tex0,	// space
                        tex1,	// maski
			tex2;	// src pl.: cam
 uniform vec2		tgPX;
 uniform vec2		aTX[8];
 uniform vec4 		aCNL[8];
 
 vec2 dis( vec2 fUV, vec2 cnl ){
      return abs(fUV*tgPX - cnl );
 }
 float l( vec2 fUV, vec2 cnl ) {
       return length(dis(fUV,cnl));
 }
 float mn( vec2 fUV, vec2 cnl ) {
       vec2 d = dis(fUV,cnl);
       return min(d.x,d.y);
 }
 vec3 line( vec2 fUV, vec2 c0, vec2 c1 ) {
       vec2 px = fUV*tgPX - c0,
            c01 = c1-c0;
       float l = length(c01),
             pxl = dot( px, c01/l ),
             pxm = length(px-(c01*pxl/l));

       return vec3( pxm, pxl, l );
 }
 void main()
 {
      vec2	fUV = fr_uv*vec2(1.0,-1.0) + vec2(0.0,1.0),
                cUV = aCNL[0].xy/tgPX,
		crs = abs(fUV-cUV)*tgPX;

      gl_FragColor = texture2D( tex2, fUV )
                   + texture2D( tex1, fUV );
      vec2 d = dis(fUV,aCNL[0].xy);
      float r = length(d), rr = abs(r-30);
      gl_FragColor.r = rr < 1 ? 1.0 : gl_FragColor.r;
      if( min(d.x,d.y) < 1.0 && length(d)<10.0 )
          gl_FragColor.r = 1.0;

      d = dis(fUV,aCNL[1].xy);
      r = length(d), rr = abs(r-30);
      gl_FragColor.g = rr < 1 ? 1.0 : gl_FragColor.g;
      if( min(d.x,d.y) < 1.0 && length(d)<10.0 )
          gl_FragColor.g = 1.0;

      vec3 xyz = line( fUV,aCNL[0].xy,aCNL[1].xy);
      if( xyz.x < 1 && xyz.y > -1 && xyz.y-1 < xyz.z )
          gl_FragColor.g = 1.0;

      d = dis(fUV,aCNL[2].xy);
      r = length(d), rr = abs(r-30);
      gl_FragColor.g = rr < 1 ? 1.0 : gl_FragColor.g;
      if( min(d.x,d.y) < 1.0 && length(d)<10.0 )
          gl_FragColor.g = 1.0;

      d = dis(fUV,aCNL[3].xy);
      r = length(d), rr = abs(r-30);
      gl_FragColor.g = rr < 1 ? 1.0 : gl_FragColor.g;
      if( min(d.x,d.y) < 1.0 && length(d)<10.0 )
          gl_FragColor.g = 1.0;
 }

 ";
 t=ms;
 again=1000;

